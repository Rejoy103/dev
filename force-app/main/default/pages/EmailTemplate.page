<apex:page standardController="Account">
    <apex:pageBlock >
    	<apex:pageBlockTable value="{!Account.contacts}" var="con">
        	<apex:column value="{!con.Name}"/>
            <apex:column value="{!con.Phone}"/>
            <apex:column value="{!con.Email}"/>
        </apex:pageBlockTable>
    </apex:pageBlock>
</apex:page>